MAEA;AACA,UAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,GAAA;AACA;AACA,OANA;AAOA;AATA;AAtBA,CAAA","sourcesContent":["<template>\r\n  <div style=\"margin-bottom: 100px;\">\r\n    <div style=\"margin: 20px 0\">\r\n      <span style=\"font-size: 20px; color: #333\">{{exam.name}}</span>\r\n      <span style=\"font-size: 14px; color: #333; margin-left: 20px\">开始时间：{{exam.time}}</span>\r\n      <span style=\"font-size: 14px; color: #333; margin-left: 20px\">持续时间：{{exam.duration}}分</span>\r\n    </div>\r\n    <div style=\"margin: 10px 0\">\r\n      <el-card>\r\n        <div v-for=\"(item,index) in questions\" :key=\"item.id\" style=\"margin: 10px 0\">\r\n          <div style=\"margin: 10px 0; font-size:20px\" ><span>{{index+1}}.</span><b>{{item.name}}</b>\r\n            <span style=\"font-size: 14px\" v-if=\"item.type ===1\">(选择题)</span>\r\n            <span style=\"font-size: 14px\" v-if=\"item.type ===2\">(判断题)</span>\r\n            <span style=\"font-size: 14px\" v-if=\"item.type ===3\">(问答题)</span>\r\n          </div>\r\n          <div v-if=\"item.type === 1\" style=\"margin: 10px\">\r\n            <span style=\"margin-right: 20px\"><el-radio v-model=\"item['studentAnswer']\" label=\"A\">A.{{item.a}}</el-radio></span>\r\n            <span style=\"margin-right: 20px\"><el-radio v-model=\"item['studentAnswer']\" label=\"B\">B.{{item.b}}</el-radio></span>\r\n            <span style=\"margin-right: 20px\"><el-radio v-model=\"item['studentAnswer']\" label=\"C\">C.{{item.c}}</el-radio></span>\r\n            <span style=\"margin-right: 20px\"><el-radio v-model=\"item['studentAnswer']\" label=\"D\">D.{{item.d}}</el-radio></span>\r\n          </div>\r\n          <div v-if=\"item.type === 2\" style=\"margin: 10px\">\r\n            <span style=\"margin-right: 20px\"><el-radio v-model=\"item['studentAnswer']\" label=\"是\">是</el-radio></span>\r\n            <span style=\"margin-right: 20px\"><el-radio v-model=\"item['studentAnswer']\" label=\"否\">否</el-radio></span>\r\n          </div>\r\n          <div v-if=\"item.type === 3\" style=\"margin: 10px\">\r\n            <el-input type=\"textarea\" v-model=\"item['studentAnswer' + index]\"></el-input>\r\n          </div>\r\n<!--          <div style=\"margin: 10px\" >-->\r\n<!--            答案：{{item.answer}}-->\r\n<!--          </div>-->\r\n<!--          <div style=\"margin: 10px\" >-->\r\n<!--            解析：{{item.detail}}-->\r\n<!--          </div>-->\r\n        </div>\r\n      </el-card>\r\n      <div style=\"margin: 20px; text-align: center\">\r\n        <el-button size=\"medium\" type=\"primary\" @click=\"submitPaper\">提交</el-button>\r\n        <el-button size=\"medium\" type=\"warning\" @click=\"$router.push('/front/home')\">取消</el-button>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"Paper\",\r\n  data() {\r\n    return {\r\n      questions: [],\r\n      examId:  this.$route.query.examId,\r\n      exam: [],\r\n    }\r\n  },\r\n  created() {\r\n    this.request.get(\"/exam/\" + this.examId).then(res =>{\r\n      this.exam = res.data\r\n    })\r\n\r\n    this.request.get(\"/examPaper/exam/\" + this.examId).then(res => {//根据考试ID查询试卷Id,根据试卷Id查询所有试题\r\n      if (res.data) {\r\n        this.request.get(\"/paper/view/\" + res.data.paperId).then(res =>{\r\n          this.questions=res.data\r\n        })\r\n      }\r\n    })\r\n  },\r\n  methods: {\r\n    submitPaper() {\r\n      this.request.post(\"/studentPaper/\",{examId: this.examId , paper: JSON.stringify(this.questions)}).then(res =>{\r\n        if (res.code === '200'){\r\n          this.$message.success(\"提交成功!\")\r\n        }else {\r\n          this.$message.error(res.msg)\r\n        }\r\n      })\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n\r\n</style>"],"sourceRoot":"src/views/front"}]}